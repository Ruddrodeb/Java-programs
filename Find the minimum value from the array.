public class PracticeJava {
    public static void main(String[] args) {
        int [] arr = { 1, 2, 3, 4, 5, 6, 7, 8};
        int min = Integer.MAX_VALUE;
        for(int e:arr){
            if(e<min){
                min =e;
            }
        }
        System.out.println(" The value of the minimum in the arry is" + " = " + min);
    }
}
