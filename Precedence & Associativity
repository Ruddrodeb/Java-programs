public class OpPre {
    public static void main(String[] args) {
        int a = 6*5-34/2;
        int b = 60/5-34*2;
        System.out.println(a);
        /* 30-34/2
        =30-17
        =13
        As it will go as Precedence
         */
        System.out.println(b);
        /*
        12-34*2
        =12-68
        =-56
        As it will go as Associativity (Left to right)
         */

        // Precedence & Associativity
    }
}
