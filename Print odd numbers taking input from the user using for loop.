import java.util.Scanner;

public class for_loop {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        System.out.println("How many odd numbers do you want to print?");
        int n = sc.nextInt();

        for(int i=0; i<n; i++){
            System.out.println(2*i+1);
        }
    }
}
