public class PracticeJava {
    public static void main(String[] args) {
        int [] arr = { 1, 2, 3, 4, 5, 6, 7, 8};
        int max =0;
        for(int e:arr){
            if(e>max){
                max =e;
            }
        }
        System.out.println(" The value of the maximum in the arry is" + " = " + max);
    }
}
