public class PracticeJava {
    public static void main(String[] args) {
        // Practice problem oneb
        float [] marks = { 45.5f, 34.5f, 34.4f, 22.3f,34.4f};
        float sum = 0;
        for(float element:marks){
            sum = sum + element;

        }
        System.out.println("The value of sum is " + sum);
    }
}
